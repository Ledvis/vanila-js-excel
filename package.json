{
  "name": "excel-course",
  "version": "1.0.0",
  "description": "SPA excel application",
  "main": "webpack.config.js",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore",
      "fix": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ledvis/vanila-js-excel.git"
  },
  "keywords": [
    "js",
    "javascript",
    "excel"
  ],
  "author": "Fedyai Eugene <fedyai.eugen@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ledvis/vanila-js-excel/issues"
  },
  "homepage": "https://github.com/Ledvis/vanila-js-excel#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/preset-env": "^7.10.2",
    "@testing-library/jest-dom": "^5.11.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.3",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.6",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.4.2",
    "jest-runner-eslint": "^0.10.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.2.1",
    "sass": "^1.26.8",
    "sass-loader": "^13.2.0",
    "stylelint": "^13.6.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.17.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "normalize.css": "^8.0.1",
    "reset-css": "^5.0.1",
    "string-similarity": "^4.0.4"
  }
}
